{
    "name": "kodix/laravel-helpers",
    "description": "Laravel kodix helpers with classes and function used for common features in projects",
    "type": "library",
    "require": {
        "malyusha/laravel-webpack-assets": "^1.0"
    },
    "require-dev": {
        "laravel/framework": "^5.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Malyuk",
            "email": "im@kodix.ru"
        }
    ],
    "autoload": {
        "psr-4": {
            "Kodix\\LaravelHelpers\\": "src"
        },
        "files": [
            "lib.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "aliases": {
            },
            "providers": [
                "Kodix\\LaravelHelpers\\Providers\\KodixServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
